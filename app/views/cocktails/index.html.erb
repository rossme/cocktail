<div class="container">
  <div class="row">
    <div class="col">
      <div class="h3">
        <% @cocktails.each do |cocktail| %>
          <div class="h3" style="margin-left: 30%; padding-top: 8px; text-align: right;">
          <%= link_to cocktail.name, cocktail_path(cocktail)%>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>